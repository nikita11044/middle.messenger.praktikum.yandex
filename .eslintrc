{
    "extends": ["airbnb", "airbnb-typescript/base"],
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "rules": {
        "no-param-reassign": "off",
        "no-underscore-dangle": "off",
        "prefer-destructuring": "off",
        "max-len": "off"
    }
}
