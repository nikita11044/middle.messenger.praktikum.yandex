    "@typescript-eslint/parser": "5.27.1",
    "eslint": "8.17.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "express": "4.18.1",
    "parcel": "2.6.0",
    "parcel-plugin-handlebars": "0.4.4",
    "postcss": "8.4.14",
    "postcss-brand-colors": "3.0.0",
    "postcss-nested": "5.0.6",
    "postcss-simple-vars": "6.0.3",
    "typescript": "4.7.3"
  },
  "dependencies": {
    "handlebars": "4.7.7",
    "nanoid": "4.0.0"
