{
  "name": "telechat",
  "version": "0.0.1",
  "scripts": {
    "eslint": "eslint \"**/*.ts\" --ignore-pattern node_modules/\n\n",
    "stylelint": "stylelint src/**/*.scss",
    "stylelint:fix": "npx stylelint src/**/*.scss --fix",
    "watch": "parcel src/index.html --no-cache && parcel watch ./src/*",
    "build": "parcel build src/index.html --no-cache --public-url ./",
    "start": "npm run build && node ./server.js"
  },
  "devDependencies": {
    "@anikin/parcel-transformer-handlebars": "1.0.9",
    "@anikin/parcel-transformer-handlebars-precompile": "1.0.1",
    "@parcel/transformer-sass": "2.6.0",
    "@types/chai": "4.3.1",
    "@types/jsdom": "16.2.14",
    "@types/mocha": "9.1.1",
    "@types/sinon": "10.0.12",
    "@types/sinon-chai": "3.2.8",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "chai": "4.3.6",
    "eslint": "8.17.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "express": "4.18.1",
    "jsdom": "20.0.0",
    "mocha": "10.0.0",
    "parcel": "2.6.0",
    "parcel-plugin-handlebars": "0.4.4",
    "sinon": "14.0.0",
    "sinon-chai": "3.7.0",
    "stylelint": "14.9.1",
    "stylelint-config-standard-scss": "5.0.0",
    "stylelint-scss": "4.3.0",
    "ts-node": "10.9.1",
    "typescript": "4.7.3"
  },
  "dependencies": {
    "handlebars": "4.7.7",
    "uuid": "8.3.2"
  }
}
